---
import IconButton from "./IconButton.astro";
export type Props = {
  alt: string;
  human?: string;
  href: string;
  icon: string;
};
let { alt, human, href, icon } = Astro.props;
---

<IconButton As="a" asProps={{ href, target: "_blank", rel: "noopener noreferrer" }} alt={alt} human={human} icon={icon} />
